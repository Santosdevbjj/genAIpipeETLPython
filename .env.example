# .env.example
SPRING_PROFILES_ACTIVE=dev

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=santander
DB_USER=postgres
DB_PASSWORD=postgres

# Security (JWT)
JWT_ISSUER=http://localhost/issuer
JWT_AUDIENCE=genai-api
# Coloque a chave pública PEM (uma linha) ou use variável segura no ambiente CI/CD
JWT_PUBLIC_KEY=-----BEGIN PUBLIC KEY-----MIIBIjANBgkqhkiG9w0BAQEFAAOCAQ8A...-----END PUBLIC KEY-----

# IA Provider (opcional, usado pelo ETL)
AI_PROVIDER=mock
AI_MODEL=gpt-4o-mini
AI_API_KEY=replace-with-your-key

# API base para ETL
API_BASE=http://localhost:8080/api
